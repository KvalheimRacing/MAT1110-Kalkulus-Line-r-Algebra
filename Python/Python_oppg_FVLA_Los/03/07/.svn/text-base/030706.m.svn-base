% Oppgave 3.7.6
r=-2:0.02:2;
s=-2:0.02:2;
[x,y]=meshgrid(r,s);
z=x.^2.*y./(x.^2+y.^2);
figure(6)
mesh(x,y,z)
hold on
t=linspace(0,2,100);
plot3(t,t.^2,0.5*ones(1,length(t)))
title('Oppgave 3.7.6')
hold off
figure(7)
contour(x,y,z);
title('Oppgave 3.7.6')
contour(x,y,z,[0.5 0.5]);
title('Oppgave 3.7.6')
