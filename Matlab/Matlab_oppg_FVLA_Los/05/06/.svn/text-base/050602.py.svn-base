from numpy import *
from newtonfler import *

newtonfler(array([0.5,0.5]),\
          lambda x : array([x[0]**2-x[1],x[0]**2+x[1]**2-1]),\
          lambda x : matrix([[2*x[0],-1],[2*x[0],2*x[1]]]))
          
newtonfler(array([-0.5,0.5]),\
          lambda x : array([x[0]**2-x[1],x[0]**2+x[1]**2-1]),\
          lambda x : matrix([[2*x[0],-1],[2*x[0],2*x[1]]]))