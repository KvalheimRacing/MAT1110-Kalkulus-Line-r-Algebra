from integrate2D import *

# Oppgave 6.3.2 a)
print integrate2D(lambda r,theta: (r**4)*cos(theta)*(sin(theta)**2),0,3,0,pi/2,100,100)

# Oppgave 6.3.2 b)
print integrate2D(lambda r,theta: (r**3),0,5,0,pi/4,100,100)

# Oppgave 6.3.2 c)
print integrate2D(lambda r,theta: r*exp(r**2),1,4,0,2*pi,100,100)

# Oppgave 6.3.2 d)
print integrate2D(lambda r,theta: r**3*sin(theta)*cos(theta),0,1,0,pi/4,100,100)

# Oppgave 6.3.2 e) 
print integrate2D(lambda x,y: (x**2-y**2)*(x**2+y**2<=1)*(y>=sqrt(3)*x)*(y<=sqrt(3)*x/3),-1,0,-1,0,100,100)

# Oppgave 6.3.2 f)
print integrate2D(lambda r,theta: r*sqrt(2-(r*cos(theta))**2-(r*sin(theta))**2),0,1,0,pi/2,100,100)

# Oppgave 6.3.2 g)
print integrate2D(lambda x,y: (x**2+y**2)**(3.0/2.0)*((x-1)**2 +y**2<=1),0,2,-1,1,100,100)
